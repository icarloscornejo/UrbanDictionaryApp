<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.prototype.urbandictionary">

    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:name="com.prototype.urbandictionary.UrbanDictionaryApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_ub_blue"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ub_blue_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UrbanDictionary"
        tools:ignore="AllowBackup">
        <activity android:name=".presentation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>